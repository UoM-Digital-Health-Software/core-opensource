<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Question.
    -->
    <changeSet id="20210716170937-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="rel_question__question_item"
                                 constraintName="fk_rel_question__question_item__question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question"/>

        <addForeignKeyConstraint baseColumnNames="question_item_id"
                                 baseTableName="rel_question__question_item"
                                 constraintName="fk_rel_question__question_item__question_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question_item"/>

        <addForeignKeyConstraint baseColumnNames="question_page_id"
                                 baseTableName="question"
                                 constraintName="fk_question__question_page_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question_page"/>
    </changeSet>
</databaseChangeLog>
